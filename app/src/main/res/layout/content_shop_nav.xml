<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".ShopNav"
    tools:showIn="@layout/app_bar_shop_nav">

    <FrameLayout
        android:id="@+id/content"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageButton
            android:id="@+id/coverimg"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:adjustViewBounds="true"
            android:background="@drawable/shoppingcover"
            android:contentDescription="@string/app_name" />

        <ImageView
            android:id="@+id/womenimg"
            android:layout_width="210dp"
            android:layout_height="match_parent"
            android:layout_below="@+id/coverimg"
            android:layout_marginTop="210dp"
            android:adjustViewBounds="true"
            android:background="@drawable/womencover"
            android:contentDescription="@string/app_name" />

        <ImageView
            android:id="@+id/menimg"
            android:layout_width="210dp"
            android:layout_height="match_parent"
            android:layout_below="@+id/coverimg"
            android:layout_marginLeft="205dp"
            android:layout_marginTop="210dp"
            android:adjustViewBounds="true"
            android:background="@drawable/mencover"
            android:contentDescription="@string/app_name" />


    </FrameLayout>


</RelativeLayout>